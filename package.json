{
  "name": "vue-formor",
  "version": "3.2.1",
  "description": "Form validation for Vue in composition functions with Yup or Zod.",
  "main": "./dist/vue-formor.cjs",
  "module": "./dist/vue-formor.js",
  "types": "./dist/src/vue-formor.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-formor.js",
      "require": "./dist/vue-formor.cjs",
      "types": "./dist/src/vue-formor.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "unit": "vitest --run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vanilla-IceCream/vue-formor.git"
  },
  "keywords": [
    "vue",
    "composition",
    "form",
    "validation",
    "yup",
    "zod",
    "schema"
  ],
  "author": "Vanilla IceCream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vanilla-IceCream/vue-formor/issues"
  },
  "homepage": "https://github.com/Vanilla-IceCream/vue-formor#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.1",
    "@vue/test-utils": "^2.4.1",
    "happy-dom": "^10.9.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.1",
    "vitest": "^0.34.1",
    "vue": "^3.3.4",
    "yup": "^1.2.0",
    "zod": "^3.21.4"
  }
}
