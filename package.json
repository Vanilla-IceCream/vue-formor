{
  "name": "vue-formor",
  "version": "5.0.0",
  "description": "Form validation for Vue in composition functions with Valibot or Zod or Yup.",
  "main": "./dist/vue-formor.cjs",
  "module": "./dist/vue-formor.js",
  "types": "./dist/src/vue-formor.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/vue-formor.d.ts",
        "default": "./dist/vue-formor.js"
      },
      "require": {
        "types": "./dist/src/vue-formor.d.ts",
        "default": "./dist/vue-formor.cjs"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "unit": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vanilla-IceCream/vue-formor.git"
  },
  "keywords": [
    "vue",
    "composition",
    "form",
    "validation",
    "valibot",
    "zod",
    "yup",
    "schema"
  ],
  "author": "Vanilla IceCream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vanilla-IceCream/vue-formor/issues"
  },
  "homepage": "https://github.com/Vanilla-IceCream/vue-formor#readme",
  "devDependencies": {
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^14.12.0",
    "typescript": "^5.4.5",
    "valibot": "^0.33.1",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue": "^3.4.29",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  }
}
